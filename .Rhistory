select(participant, correctAns, rotation, key_resp.keys) |>
na.omit()
store |>
mutate(acc = if_else(correctAns == key_resp.keys, 1, 0)) |>
count(participant, rotation, acc) |>
filter(acc == 1)
}
out <- do.call('rbind.data.frame', lapply(xx, take_data))
View(out)
out |> filter(rotation == 0)
out |> filter(rotation == 1)
out |> filter(rotation == 180)
