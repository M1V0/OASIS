#reorder by entry_n
pData_AOI_first <- pData_AOI_first[order(pData_AOI_first$entry_n),]
#keep only the first from each trial
pData_AOI_first <- pData_AOI_first[with(pData_AOI_first,
ave(start, trial,
FUN = function(x) rank(x, ties.method = "first"))) == 1, ]
pData_AOI_first <- pData_AOI_first[order(pData_AOI_first$trial),]
rownames(pData_AOI_first) <- NULL
pData_AOI_first$end <- NULL
pData_AOI_first$duration <- NULL
pData_AOI_first$entry_n <- NULL
pData_AOI_first$AOI <- NULL
