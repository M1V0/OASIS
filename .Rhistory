block <- data.frame(
trial = 1:trials,
block = rep(1:24, each = trials/24)
)
#get the conditions from the datafiles ----
faces <- data.frame(
trial = data[['training.data']][[1]]$trial,
outcome_item = data[['training.data']][[1]]$outcome_item,
expression = NA
)
faces[faces$outcome_item %% 2 == 0,]$expression <- "neutral"
faces[faces$outcome_item %% 2 == 1,]$expression <- "fearful"
faces$outcome_item <- NULL
#combine eyes ----
