AOI_time(data = pData_dispersion,
data_type = "fix",
AOIs = AOI_AGP,
AOI_names = AOI_names_AGP)
pData_AOI_seq <- AOI_seq(data = pData_dispersion,
AOIs = AOI_AGP,
AOI_names = AOI_names_AGP)
